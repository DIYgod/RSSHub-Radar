{
  "name": "rsshub-radar",
  "version": "0.0.1",
  "description": "Browser extension to detect available RSS and RSSHub for current page",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack -w --mode=production --progress",
    "build": "webpack --mode=production --progress",
    "release": "npm run build && zip -r release/radar.zip dist",
    "format": "eslint \"**/*.js\" --fix && prettier \"**/*.{js,scss,less}\" --write",
    "format:staged": "eslint \"**/*.js\" --fix && pretty-quick --staged --verbose --pattern \"**/*.{js,scss,less}\"",
    "format:check": "eslint \"**/*.js\" && prettier-check \"**/*.{js,scss,less}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DIYgod/RSSHub-Radar.git"
  },
  "keywords": [
    "rsshub",
    "rss"
  ],
  "gitHooks": {
    "pre-commit": "npm run format:staged"
  },
  "author": "DIYgod",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DIYgod/RSSHub-Radar/issues"
  },
  "homepage": "https://github.com/DIYgod/RSSHub-Radar#readme",
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.6",
    "autoprefixer": "10.3.6",
    "babel-loader": "8.2.2",
    "babel-plugin-component": "1.1.1",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.3.0",
    "cssnano": "5.0.8",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-prettier": "4.0.0",
    "file-loader": "6.2.0",
    "less": "4.1.1",
    "less-loader": "10.0.1",
    "mini-css-extract-plugin": "2.3.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "6.1.1",
    "prettier": "2.4.1",
    "prettier-check": "2.0.0",
    "pretty-quick": "3.1.1",
    "sass-loader": "12.1.0",
    "svg-inline-loader": "0.8.2",
    "template-string-optimize-loader": "3.0.0",
    "url-loader": "4.1.1",
    "vue": "2.6.14",
    "vue-loader": "15.9.8",
    "vue-template-compiler": "2.6.14",
    "webpack": "5.54.0",
    "webpack-cli": "4.8.0",
    "yorkie": "2.0.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "clipboard": "2.0.8",
    "element-ui": "2.15.6",
    "https-browserify": "^1.0.0",
    "md5.js": "^1.3.5",
    "psl": "1.8.0",
    "route-recognizer": "0.3.4",
    "rss-parser": "3.12.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "url": "^0.11.0",
    "vue-router": "3.5.2"
  }
}
